{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended",
        "tslint-config-prettier",
        "tslint-config-airbnb"
    ],
    "rules": {
        "trailing-comma": false,
        "arrow-return-shorthand": true,
        "no-console": false,
        "prefer-const": true,
        "no-shadowed-variable": false,
        "no-empty": false,
        "no-parameter-reassignment": false
    },
    "rulesDirectory": []
}
